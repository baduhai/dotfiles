!-----URxvt Settings------

URxvt.font: 		xft:xos4 Terminus:pixelsize=18
URxvt.scrollBar: 	false
URxvt.urlLauncher: 	firefox
URxvt.cursorBlink: 	true
URxvt.geometry:		80x23
URxvt.letterSpace:	-1
URxvt.depth: 32

! special
