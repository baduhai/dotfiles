!-----URxvt Settings------

URxvt.font: 		xft:Fira Mono:pixelsize=16
URxvt.scrollBar: 	false
URxvt.urlLauncher: 	firefox
URxvt.cursorBlink: 	true
URxvt.geometry:		80x23
URxvt.letterSpace:	-1
URxvt.depth: 32

! special
*.foreground:   #aaaaaa
*.background:   [90]#414141
*.cursorColor:  #aaaaaa

! black
*.color0:       #1e1f29
*.color8:       #555753

! red
*.color1:       #ff512f
*.color9:       #ff6e52

! green
*.color2:       #b2ffa9
*.color10:      #cbffc5

! yellow
*.color3:       #fffd82
*.color11:      #fffea8

! blue
*.color4:       #3185fc
*.color12:      #579bfc

! magenta
*.color5:       #dd2476
*.color13:      #dd4e8e

! cyan
*.color6:       #66d7d1
*.color14:      #93d7d3

! white
*.color7:       #c7c5c1
*.color15:      #f2efea
