! green
*.color2:       #b2ffa9
*.color10:      #b2ffa9

! yellow
*.color3:       #fffd82
*.color11:      #fffd82

! blue
*.color4:       #3185fc
*.color12:      #3185fc

! magenta
*.color5:       #dd2476
*.color13:      #dd2476

! cyan
*.color6:       #66d7d1
